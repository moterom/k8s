metadata:
  name: deploy-api-foodgroups
spec:
  selector:
    matchLabels:
      app: api-foodgroups
  replicas: 1
  template:
    metadata:
      labels:
        app: api-foodgroups
    spec:
      containers:
      - name: api-foodgroups
        image: 862980326144.dkr.ecr.us-east-1.amazonaws.com/api-foodgroups:dev-IMAGE_TAG
        resources:
          requests:
            memory: "128Mi"
            cpu: "125m"
        ports:
        - containerPort: 5012