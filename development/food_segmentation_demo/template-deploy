  selector:
    matchLabels:
      app: food-segmentation-demo
  replicas: 1
  template:
    metadata:
      labels:
        app: food-segmentation-demo
    spec:
      containers:
      - name: food-segmentation-demo
        image: 862980326144.dkr.ecr.us-east-1.amazonaws.com/food_segmentation_demo:dev-IMAGE_TAG
        resources:
          requests:
            memory: "128Mi"
            cpu: "250m"
        ports:
        - containerPort: 5337
