apiVersion: apps/v1
kind: Deployment
metadata:
  name: deploy-api-notifications
spec:
  selector:
    matchLabels:
      app: api-notifications
  replicas: 1
  template:
    metadata:
      labels:
        app: api-notifications
    spec:
      containers:
      - name: api-notifications
        image: 862980326144.dkr.ecr.us-east-1.amazonaws.com/api_notifications:dev-IMAGE_TAG
        resources:
          requests:
            memory: "128Mi"
            cpu: "125m"
        ports:
        - containerPort: 5006
